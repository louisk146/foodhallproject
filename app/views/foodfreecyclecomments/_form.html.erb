<%= form_for([@foodfreecycle, @foodfreecycle.foodfreecyclecomments.build]) do |f| %>
  
  <p>
    <%= f.label :commenter %><br>
    <%= f.text_field :commenter %>
  </p>
  <p>
    <%= f.label :body %><br>
    <%= f.text_area :body %>
  </p>

	<p>
    <%= f.label :contact %><br>
    <%= f.text_area :contact %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>